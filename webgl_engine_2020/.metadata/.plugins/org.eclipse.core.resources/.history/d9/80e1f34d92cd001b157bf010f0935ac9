package get_old_part_rude;

import kernel_common_class.debug_information;
import kernel_file_manager.file_reader;
import kernel_file_manager.file_writer;

public class test 
{
	public static void main(String args[])
	{
		debug_information.println("Begin");
		
		file_reader fr=new file_reader("e:\\t.mesh","GBK");
		file_writer fw=new file_writer("e:\\t.txt","GBK");
		file_writer face_fw=new file_writer("e:\\t.txt.face","GBK");
		file_writer edge_fw=new file_writer("e:\\t.txt.edge","GBK");
		
		part_rude pr=new part_rude(fr,1,1,false,false,false);
		pr.write_out(fw);
		
		auxiliary_file_handler afh=new auxiliary_file_handler("e:\\t.mesh","GBK");
		pr.write_out_face(face_fw,afh);
		pr.write_out_edge(edge_fw,afh);
		
		fr.close();
		fw.close();
		face_fw.close();
		edge_fw.close();

		debug_information.println("End");
	}
}
