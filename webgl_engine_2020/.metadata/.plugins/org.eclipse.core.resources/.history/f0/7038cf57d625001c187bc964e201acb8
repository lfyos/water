package kernel_maintain;

import kernel_common_class.debug_information;
import kernel_file_manager.file_reader;

public class test_3
{
	private static void process(String file_name)
	{
		String file_array[]=new String[] {
				null,null,null,null,null,null,null,null,null,null,null,
				null,null,null,null,null,null,null,null,null,null,null,
				null,null,null,null,null,null,null,null,null,null,null,
				null,null,null,null,null,null,null,null,null,null,null,
				null,null,null,null,null,null,null,null,null,null,null
		};
		file_reader f=new file_reader(file_name,null);
		for(int i=0;i<18;i++)
			f.get_string();
		int number=0;
		while(!(f.eof())) {
			String str1=f.get_string();
			f.get_string();
			String str2=f.get_string();
			if(str2==null)
				continue;
			if(str1.compareTo("part_list")==0)
				file_array[number++]=str2;
		}
		
		
	}
	public static void main(String args[])
	{
		debug_information.println("Begin");
		
		for(file_reader f1=new file_reader("E:\\water_all\\data\\users\\users\\NoName\\assemble.txt",null);;) {
			if(f1.eof()) {	
				f1.close();
				break;
			}
			
			String file_name=f1.get_string();
			if(file_name==null)
				continue;
			file_name=file_reader.separator(f1.directory_name+file_name);
			for(file_reader f2=new file_reader(file_name,null);;) {
				if(f2.eof()) {	
					f2.close();
					break;
				}
				f2.get_string();
				f2.get_string();
				file_name=f2.get_string();
				f2.get_string();
				f2.get_string();
				f2.get_string();
				if(f2.get_string()!=null)
					process(file_reader.separator(f2.directory_name+file_name));
			}
		}
		debug_information.println("End");
	}
}
