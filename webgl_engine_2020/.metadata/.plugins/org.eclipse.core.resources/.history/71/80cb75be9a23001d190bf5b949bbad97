<canvas id="my_canvas"	tabindex="0" width=1000	 height=500></canvas>


<script type="text/javascript">

function body_onload()
{
	if(typeof(navigator.gpu)=="undefined")
		alert("undefined");
	else if(navigator.gpu==null)
		alert("null");
	else
		alert("success");
}


</script>


