<html>
<head>

<script type="text/javascript" 	src="./interface.jsp?function_name=construct_render_object"></script>
<script type="text/javascript">

var render;
function body_onload()
{
	construct_render_object(
		document.getElementById("my_canvas"),		//绘图画布
		"NoName",									//用户名
		"NoPassword",								//用户密码
		"chinese",									//语言
		"bridge_old_32",							//场景名称	bridge_old_32		 64_old_one_16		64_old_two_24
		"",											//连接名称,用于多人协同操作一个场景
		[											//初始化参数
		],
		function(my_render)					//创建场景成功后执行的函数
		{
			render=my_render;				//把场景变量保存到变量render中
			document.title=render.title;	//设置网页标题
			
			render.zhengzhou_lipengfei.turn_on_off_display_point_on_pickup(false);
			
			render.zhengzhou_lipengfei.register_collector_stack_monitor(	//注册列表监视器
				function(jason_data,text_data)
				{
//					console.clear();
//					console.log(text_data);
				});
		});
}

function body_onunload()
{
	if(typeof(render)=="object")
		if(render!=null)
			if(typeof(render.terminate)=="function")
				render.terminate();
}


</script>
</head>

<body onload="body_onload();" onunload="body_onunload();">
<canvas id="my_canvas"	tabindex="0" width=1000	 height=500></canvas>
<br/>

<button type="button" onclick="render.zhengzhou_lipengfei.turnonoff_sound(true);"		>打开配音</button>
<button type="button" onclick="render.zhengzhou_lipengfei.turnonoff_sound(false);"		>关闭配音</button>
<button type="button" onclick="render.zhengzhou_lipengfei.turnonoff_lod(true,true);"	>打开层次细节</button>
<button type="button" onclick="render.zhengzhou_lipengfei.turnonoff_lod(false,false);"	>关闭层次细节</button>
<br/>

<button type="button" onclick="render.zhengzhou_lipengfei.set_display_mounting_component();"	>正在拼装组件</button>
<button type="button" onclick="render.zhengzhou_lipengfei.set_display_mounted_component();" 	>已经拼装组件</button>
<button type="button" onclick="render.zhengzhou_lipengfei.set_display_unmounted_component();"	>尚未拼装组件</button>
<button type="button" onclick="render.zhengzhou_lipengfei.set_display_all_component();" 		>所有拼装组件</button>
<br/>

<button type="button" onclick="render.zhengzhou_lipengfei.mount_forward(true);"		>单步向前拼装</button>
<button type="button" onclick="render.zhengzhou_lipengfei.mount_forward(false);"	>连续向前拼装</button>
<button type="button" onclick="render.zhengzhou_lipengfei.mount_backward(true);"	>单步向后拆卸</button>
<button type="button" onclick="render.zhengzhou_lipengfei.mount_backward(false);"	>连续向后拆卸</button>
<button type="button" onclick="render.zhengzhou_lipengfei.mount_stop();"			>停止拼装</button>
<button type="button" onclick="render.zhengzhou_lipengfei.mount_jump(prompt());"	>拼装跳转</button>
<br/> 

<button type="button" onclick="window.open(render.zhengzhou_lipengfei.mount_all_url(),'_blank');" 		>拼装jason</button>
