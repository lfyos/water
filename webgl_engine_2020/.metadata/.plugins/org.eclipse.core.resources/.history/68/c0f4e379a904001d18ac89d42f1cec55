package kernel_engine;

import java.util.concurrent.locks.ReentrantLock;

public class engine_kernel_and_client_information_container
{
	private boolean initilization_flag;
	private ReentrantLock engine_kernel_lock;
	public engine_kernel ek;
	
	public client_information 		client_information;
	public volatile int 			lock_number;
	
	public engine_kernel_and_client_information_container()
	{
		client_information	=null;
		lock_number			=0;
	}
}
