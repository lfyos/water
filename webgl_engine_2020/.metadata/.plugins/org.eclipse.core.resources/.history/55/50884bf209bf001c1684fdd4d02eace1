function construct_modifier_time_parameter(modifier_container_number)
{
	this.delay_time_length		=0;
	this.webserver_current_time	=0;
	this.timer_version			=0;
	this.timer_adjust_value		=0;

	this.caculate_current_time=function()
	{
		return this.webserver_current_time-this.timer_adjust_value;
	};
	this.modify_parameter=function(response_data)
	{
		this.delay_time_length		 =response_data[0];
		this.webserver_current_time	+=Math.round(response_data[1]);
		this.timer_version			+=Math.round(response_data[2]);
		this.timer_adjust_value		+=Math.round(response_data[3]);
		
		return this.caculate_current_time();
	};
	this.destroy=function()
	{
		this.modify_parameter		=null;
		this.caculate_current_time	=null;
		this.destroy				=null;
	};
};
